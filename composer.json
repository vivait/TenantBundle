{
    "name": "vivait/tenant-bundle",
    "description": "Tenant Bundle for Symfony, allows different environments to load based on various locators.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lewis Wright",
            "email": "lewis@vivait.co.uk"
        }
    ],
    "bin": [
        "bin/tenant"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^3.7|^5.4",
        "phpspec/phpspec": "^2.0",
        "behat/behat": "^3.0",
        "behat/symfony2-extension": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "Vivait\\TenantBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "test\\Vivait\\TenantBundle\\": ""
        }
    }
}
